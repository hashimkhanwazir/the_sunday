
public class testTime {

	public static void main(String []arg)
	{
		Time object1=new Time(10,20,25);
		System.out.println(object1.getHour());
		System.out.println(object1.getMinute());
		System.out.println(object1.getSecond());
		object1.setTime(23,34,45);
	
		
		Time object2=new Time();
		object2.setHour(11);
		object2.setMinute(45);
		object2.setSecond(56);
		System.out.println(object2.getHour());
		System.out.println(object2.getMinute());
		System.out.println(object2.getHour());
		
		object2.toString();
		System.out.println(""+object2);
		System.out.println("the same time is "+object2.toString()+" while that for "
				+ "object1 is"+object1.toString());
		
		
		
		
		
		
		
		
		
	}
}
